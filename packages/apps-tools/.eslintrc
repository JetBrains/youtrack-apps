{
  "parserOptions": {},
  "plugins": [],
  "extends": ["prettier"],
  "env": {
    "node": true,
    "es2020": true,
    "jest": true
  },
  "rules": {
    "one-var": [
      2,
      "never"
    ],
    "camelcase": 2,
    "new-cap": 0,
    "no-unreachable": 2,
    "no-extra-boolean-cast": 2,
    "valid-jsdoc": 0,
    "no-lonely-if": 2,
    "no-bitwise": 2,
    "no-div-regex": 2,
    "curly": 0,
    "eqeqeq": 2,
    "yoda": 2,
    "no-caller": 2,
    "no-dupe-args": 2,
    "no-debugger": 2,
    "no-console": 0,
    "no-shadow-restricted-names": 2,
    "no-label-var": 2,
    "no-delete-var": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-undef-init": 2,
    "handle-callback-err": 2
  }
}
