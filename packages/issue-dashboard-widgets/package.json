{
  "name": "youtrack-issues-list-widget",
  "version": "1.0.0",
  "private": true,
  "config": {
    "dist": "./dist"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider webpack serve",
    "build": "rm -rf dist && NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "upload": "youtrack-app upload dist",
    "test": "karma start",
    "lint": "eslint . --quiet=true && npm run stylelint",
    "stylelint": "stylelint --ignore-path .gitignore **/*.css",
    "ci-test": "npm run lint && xvfb-maybe karma start --teamcity",
    "i18n:extract": "angular-gettext-cli --files \"{src/**/*.js,node_modules/hub-dashboard-addons/components/**/*.js,node_modules/@jetbrains/hub-widget-ui/src/**/*.js}\" --dest src/app/translations/default.pot --marker-name=i18n --module-method-plural=plural --module-name=i18n"
  },
  "babel": {
    "presets": "@jetbrains/jetbrains",
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "@jetbrains/babel-preset-jetbrains": "1.0.1",
    "@jetbrains/hub-widget-ui": "^1.0.44",
    "@jetbrains/icons": "1.12.0",
    "@jetbrains/logos": "0.1.6",
    "@jetbrains/ring-ui": "2.0.38",
    "angular-gettext-cli": "1.2.0",
    "angular-gettext-loader": "1.0.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "core-js": "^2.6.12",
    "css-loader": "^5.2.6",
    "d3": "3.5.16",
    "date-fns": "^1.30.1",
    "diff_match_patch": "^0.1.1",
    "fecha": "3.0.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "3.2.0",
    "hub-dashboard-addons": "1.0.2",
    "json-loader": "0.5.7",
    "mobx": "^5.15.7",
    "mobx-react": "^5.4.4",
    "mout": "0.11.0",
    "nvd3": "github:JetBrains/nvd3",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.0.9",
    "postcss-modules-values-replace": "1.2.1",
    "prop-types": "15.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "~2.0.0"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "^4.0.3",
    "@jetbrains/postcss-require-hover": "^0.1.2",
    "@jetbrains/stylelint-config": "^1.3.4",
    "@jetbrains/youtrack-apps-tools": "^0.0.1-alpha.12",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "chai": "4.0.2",
    "chai-as-promised": "7.1.1",
    "chai-dom": "1.5.0",
    "chai-enzyme": "1.0.0-beta.1",
    "chai-spies": "1.0.0",
    "cheerio": "=1.0.0-rc.3",
    "copy-webpack-plugin": "4.6.0",
    "electron": "^32.1.2",
    "enzyme": "3.11.0",
    "eslint": "^5.12.1",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-bdd": "^2.1.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.10.0",
    "json-loader": "0.5.7",
    "karma": "6.3.16",
    "karma-chai-plugins": "0.9.0",
    "karma-electron-launcher": "^0.3.0",
    "karma-mocha": "2.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.1.0",
    "karma-webpack": "4.0.2",
    "minimist": "^1.2.6",
    "mocha": "9.2.0",
    "postcss-flexbugs-fixes": "^4.2.1",
    "react-test-renderer": "16.2.0",
    "sass": "^1.22.10",
    "sinon": "2.3.5",
    "sinon-chai": "2.11.0",
    "stylelint": "8.3.0",
    "svg-baker-runtime": "^1.4.7",
    "webpack": "4.47.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.2",
    "xvfb-maybe": "0.2.1"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
