{
  "name": "article-feedback",
  "title": "Article Feedback",
  "version": "0.1.5",
  "description": "Adds a widget that lets you collect and analyze reader feedback for articles in the YouTrack knowledge base.",
  "icon": "article-feedback-logo.svg",
  "dependencies": {
    "@jetbrains/youtrack-scripting-api": "*"
  },
  "vendor": {
    "name": "JetBrains",
    "url": "https://jetbrains.com"
  },
  "widgets": [
    {
      "key": "feedback-form",
      "name": "Article Feedback",
      "indexPath": "feedback-form/index.html",
      "extensionPoint": "ARTICLE_ABOVE_ACTIVITY_STREAM"
    },
    {
      "key": "feedback-statistics",
      "name": "Article Feedback Statistics",
      "indexPath": "feedback-statistics/index.html",
      "extensionPoint": "ARTICLE_OPTIONS_MENU_ITEM",
      "permissions": [
        "UPDATE_ARTICLE"
      ]
    }
  ]
}
